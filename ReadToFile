package test;
import java.io.*;
public class ReadtoFile {
	public static void main(String[] args) {
try {
	FileReader r=new FileReader("test.txt");
	try{
		int i;
		while((i=r.read())!=-1) {
			System.out.println((char)i);
		}
	}finally{
		r.close();
	}}catch(Exception e) {
		System.out.println("error");
	}
		
	}

}
